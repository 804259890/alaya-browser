<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.platon.browser.complement.mapper.ProposalBusinessMapper">
<update id="vote" parameterType="com.platon.browser.common.complement.param.proposal.ProposalText">
    insert into `vote`
    (
        `hash`,
        `node_name`,
        `node_id`,
        `option`,
        `proposal_hash`,
        `timestamp`
    )
    values
    (
        #{txHash},
        #{stakingName},
        #{nodeId},
        #{voteOption},
        #{proposalHash},
        #{timestamp}
    );

</update>
</mapper>